{
  "name": "backed-client",
  "version": "1.2.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vandeurenglenn/backed-client.git"
  },
  "keywords": [
    "build",
    "bundle",
    "serve",
    "helper",
    "custom",
    "element",
    "client",
    "backed",
    "html",
    "es",
    "plugin",
    "backed-plugin"
  ],
  "author": "Glenn Vandeuren <vandeurenglenn@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vandeurenglenn/backed-client/issues"
  },
  "homepage": "https://github.com/vandeurenglenn/backed-client#readme",
  "dependencies": {
    "backed": "^0.2.1"
  },
  "devDependencies": {
    "@webcomponents/html-imports": "github:webcomponents/html-imports",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.0",
    "rollup-plugin-babel": "^2.7.1"
  },
  "backed": {
    "bundles": [
      {
        "src": "src/backed-client.js",
        "dest": "dist/backed-client.js",
        "format": "iife",
        "moduleName": "backedClient",
        "plugins": {
          "babel": {
            "presets": [
              [
                "es2015",
                {
                  "modules": false
                }
              ]
            ],
            "plugins": [
              "external-helpers"
            ]
          },
        }
      }
    ],
    "sources": [
      {
        "src": "src/*.html",
        "dest": "dist"
      }
    ],
    "server": {
      "demo": "dist/"
    },
    "watch": [
      "src"
    ]
  }
}
